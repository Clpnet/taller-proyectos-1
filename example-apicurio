definitions:
  AuthRequest:
    type: object
    properties:
      clientId:
        type: string
        example: ebpm
        description: "Valor del Identificador del Cliente para la llamada al SSO :\
          \ client_id"
      clientSecret:
        type: string
        example: 9cd6db25-11e1-4bf2-9f1d-b2fa950bbb5d
        description: "Valor del token de secreto del cliente para la llamada al SSO\
          \ : client_secret"
      grantType:
        type: string
        example: authorization_code
        description: "Valor del tipo de autorización para la llamada al SSO : grant_type"
      scope:
        type: string
        example: openid
        description: "Valor para el scope para la llamada al SSO : scope"
      code:
        type: string
        example: be40202b-90ae-4785-a169-68dc35193e4a.4e9ca7f4-b32f-4694-966a-4acaf8bae753.57ceac33-2051-4753-9e33-313e289e3ab3
        description: "Valor del token de Código para la llamada al SSO : code"
      redirectUri:
        type: string
        example: http://localhost:4200/auth
        description: Valor de la URI a redireccionar para la llamada al SSO :redirect_uri
      responseType:
        type: string
        example: code
        description: Valor del tipo de response  para la llamada al SSO :response_type
      sessionId:
        type: string
        example: 2900df62-0eed-40c0-b69b-9f4a678a02db
        description: "Código de la sesión para la llamada al SSO : sessionId"
      refreshToken:
        type: string
        example: eyJhbG.eyJiOjE9.xhsAq5zwkQ-PNLnmX
        description: "Código del Refresh Token para la llamada al SSO : refresh_token"
    title: AuthRequest
  AuthResponse:
    type: object
    properties:
      accessToken:
        type: string
        example: eyJhbG.eyJiOjE9.xhsAq5zwkQ-PNLnmX
        description: Valor del Access token para la llamada al SSO
      expiresIn:
        type: integer
        example: 300
        description: Tiempo de Expiración del token para la llamada al SSO
      refreshExpiresIn:
        type: integer
        example: 1800
        description: Tiempo de refrescado del Token para la llamada al SSO
      refreshToken:
        type: string
        example: eyJhbG.eyJiOjE9.xhsAq5zwkQ-PNLnmX
        description: Código de Refrescado del Token para la llamada al SSO
      tokenType:
        type: string
        example: bearer
        description: Tipo de Token para la llamada al SSO
      notBeforePolicy:
        type: integer
        example: 0
        description: 'Estado ''No Before Policy'' para la llamada al SSO '
      sessionState:
        type: string
        example: 4e9ca7f4-b32f-4694-966a-4acaf8bae753
        description: Estado de Sesión para la llamada al SSO
      scope:
        type: string
        example: profile email
        description: Scope para la llamada al SSO
    title: AuthResponse
